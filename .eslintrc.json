{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-async-promise-executor": "off",
        "no-empty": "off",
        "no-unused-vars": "warn"
    }
}
